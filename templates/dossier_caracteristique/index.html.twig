{% extends 'base.html.twig' %}

{% block title %}DossierCaracteristique index{% endblock %}

{% block body %}
    <h1>DossierCaracteristique index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Description</th>
                <th>Valeur</th>
                <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dossier_caracteristique in dossier_caracteristiques %}
            <tr>
                <td>{{ dossier_caracteristique.id }}</td>
                <td>{{ dossier_caracteristique.nom }}</td>
                <td>{{ dossier_caracteristique.description }}</td>
                <td>{{ dossier_caracteristique.valeur }}</td>
                <td>{{ dossier_caracteristique.date ? dossier_caracteristique.date|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('dossier_caracteristique_show', {'id': dossier_caracteristique.id}) }}">show</a>
                    <a href="{{ path('dossier_caracteristique_edit', {'id': dossier_caracteristique.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('dossier_caracteristique_new') }}">Create new</a>
{% endblock %}
